   RowBox[{
    RowBox[{"TU_forTc", "=", 
     RowBox[{"X", "/", "4"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"Tc", "=", 
     RowBox[{"TU_forTc", "+", "Uc"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"Print", "[", 
     RowBox[{"\"\<Tc = (X/4) + Uc = \>\"", ",", 
