    RowBox[{"Print", "[", "\"\<\\n5. Deriving Ic (Theorem 19):\>\"", "]"}], 
    ";"}], "\n", 
   RowBox[{
    RowBox[{
    "Print", "[", "\"\<   Ic = UI - Uc. Assuming UI = X/4.\>\"", "]"}], ";"}],
    "\n", 
   RowBox[{
    RowBox[{"UI_forIc", "=", 
     RowBox[{"X", "/", "4"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"Ic", "=", 
     RowBox[{"UI_forIc", "-", "Uc"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"Print", "[", 
     RowBox[{"\"\<Ic = (X/4) - Uc = \>\"", ",", 
